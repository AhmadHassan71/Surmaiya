			android:id="@+id/imageView7"
			android:layout_width="21dp"
			android:layout_height="26dp"
			android:layout_marginStart="168dp"
			android:layout_marginTop="8dp"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toTopOf="parent"
			app:srcCompat="@drawable/search" />

		<ImageView
			android:id="@+id/imageView9"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginTop="8dp"
			android:layout_marginEnd="68dp"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintTop_toTopOf="parent"
			app:srcCompat="@drawable/your_lib" />

		<TextView
			android:id="@+id/your_lib"
			android:layout_width="72dp"
			android:layout_height="11dp"
			android:layout_alignParentLeft="true"
			android:layout_alignParentTop="true"
			android:layout_marginStart="340dp"
			android:layout_marginBottom="8dp"
			android:gravity="center_horizontal|top"
			android:text=" Your Library"
			android:textColor="@color/white"
			android:textSize="10sp"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintStart_toStartOf="parent" />


	</androidx.constraintlayout.widget.ConstraintLayout>

	<Button

		android:layout_width="85dp"
		android:layout_height="31dp"
		android:layout_marginEnd="56dp"
		android:layout_marginBottom="124dp"
		android:background="@drawable/green_round"

		android:text="Create"
		android:textAllCaps="false"
		android:textColor="@color/white"
		android:textSize="14dp"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent" />

	<Button

		android:layout_width="72dp"
		android:layout_height="32dp"
		android:layout_marginStart="56dp"
		android:layout_marginBottom="124dp"
		android:background="@drawable/cancel_background"
		android:text="Cancel"
		android:textAllCaps="false"
		android:textColor="@color/white"
		android:textSize="14sp"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintStart_toStartOf="parent" />

	<ScrollView
		android:id="@+id/scrollView5"

		android:layout_width="443dp"
		android:layout_height="607dp"
		android:clipChildren="false"
		android:clipToPadding="false"
		android:fillViewport="true"
		android:paddingTop="37dp"
		android:paddingBottom="0dp"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent">

		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:clipChildren="false"
			android:clipToPadding="false"
			android:orientation="vertical">

			<LinearLayout

				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginHorizontal="28dp"
				android:layout_marginBottom="67dp"
				android:orientation="horizontal">

				<ImageView

					android:layout_width="48dp"
					android:layout_height="match_parent"
					android:layout_marginEnd="79dp"
					android:scaleType="fitXY"
					android:src="@drawable/left_arrow" />

				<TextView

					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginTop="5dp"
					android:layout_weight="1"
					android:text="Add Song"
					android:textColor="#00A136"
					android:textSize="32sp" />
			</LinearLayout>

			<LinearLayout

				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginHorizontal="101dp"
				android:layout_marginBottom="11dp"
				android:background="@drawable/art_work_background"
				android:gravity="center_horizontal"
				android:orientation="vertical"
				android:paddingTop="49dp"
				android:paddingBottom="32dp">

				<TextView

					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginHorizontal="14dp"
					android:layout_marginBottom="14dp"
					android:text="Add Artwork"
					android:textColor="#303030"
					android:textSize="24sp" />

				<TextView

					android:layout_width="36dp"
					android:layout_height="wrap_content"
					android:layout_marginHorizontal="86dp"
					android:text="+"
					android:textColor="#303030"
					android:textSize="40sp" />
			</LinearLayout>

			<TextView

				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginHorizontal="120dp"
				android:layout_marginBottom="52dp"
				android:text="(300x300) minimum quality"
				android:textColor="#FFFFFF"
				android:textSize="13sp" />

			<TextView

				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginHorizontal="146dp"
				android:layout_marginBottom="27dp"
				android:text="New Song"
				android:textColor="#FFFFFF"
				android:textSize="20sp" />

			<TextView

				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginHorizontal="82dp"
				android:layout_marginBottom="5dp"
				android:text="Name"
				android:textColor="#8A9A9D"
				android:textSize="16sp" />

			<LinearLayout

				android:layout_width="266dp"
				android:layout_height="1dp"
				android:layout_marginHorizontal="62dp"
				android:layout_marginBottom="25dp"
				android:background="#FF8A9A9D"
				android:orientation="vertical"></LinearLayout>

			<TextView

				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginHorizontal="81dp"
				android:layout_marginBottom="5dp"
				android:text="Name of Artists"
				android:textColor="#8A9A9D"
				android:textSize="16sp" />

			<Vi

				android:layout_width="266dp"
				android:layout_height="1dp"
				android:layout_marginHorizontal="63dp"
				android:layout_marginBottom="43dp"
				android:background="#FF8A9A9D"
				android:orientation="vertical"></Vi>

			<TextView
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginHorizontal="150dp"
				android:text="Music File"
				android:textColor="#FFFFFF"
				android:textSize="20sp" />
		</LinearLayout>
	</ScrollView>

	<ImageView
		android:id="@+id/imageView13"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="151dp"
		android:layout_marginTop="628dp"
		android:tint="@color/icon_secondary"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:srcCompat="@drawable/add_song" />
</androidx.constraintlayout.widget.ConstraintLayout>
